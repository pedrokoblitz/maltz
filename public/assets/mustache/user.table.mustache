<!--USER-->

<table>
    <thead>
        <tr>
            <th>#</th>
            <th>username</th>
            <th>first_name</th>
            <th>middle_name</th>
            <th>last_name</th>
            <th>email</th>
            <th>activity</th>
            <th>edit</th>
            <th>delete</th>
        </tr>
    </thead>
    <tbody>

{{#users}}

<tr id="record-1" class="record">
    <td class="checkbox"><input type="checkbox" class="checkthis" /></td>
    <td class="username">{{username}}</td>
    <td class="first_name">{{first_name}}</td>
    <td class="middle_name">{{middle_name}}</td>
    <td class="last_name">{{last_name}}</td>
    <td class="email">{{email}}</td>
    <td class="activity">{{activity}}</td>
    <td class="edit">
        <p data-placement="top" data-toggle="tooltip" title="Edit">
            <button class="btn btn-primary btn-xs" data-title="Edit" data-toggle="modal" data-target="#edit" >
                <span class="glyphicon glyphicon-pencil"></span>
            </button>
        </p>
    </td>
    <td class="delete">
        <p data-placement="top" data-toggle="tooltip" title="Delete">
            <button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" >
                <span class="glyphicon glyphicon-trash"></span>
            </button>
        </p>
    </td>
</tr>

{{/users}}

    </tbody>
</table>
