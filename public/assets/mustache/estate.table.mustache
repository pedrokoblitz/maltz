<!--ESTATES-->

<table>
    <thead>
        <tr>
            <th>#</th>
            <th>place</th>
            <th>type</th>
            <th>area</th>
            <th>rooms</th>
            <th>suites</th>
            <th>parking_spots</th>
            <th>price</th>
            <th>charges</th>
            <th>taxes</th>
            <th>created</th>
            <th>edit</th>
            <th>delete</th>
        </tr>
    </thead>
    <tbody>

{{#estates}}

<tr id="record-1" class="record">
    <td class="checkbox"><input type="checkbox" class="checkthis" /></td>
    <td class="place">{{place}}</td>
    <td class="type">{{type}}</td>
    <td class="area">{{area}}</td>
    <td class="rooms">{{rooms}}</td>
    <td class="suites">{{suites}}</td>
    <td class="parking_spots">{{parking_spots}}</td>
    <td class="price">{{price}}</td>
    <td class="charges">{{charges}}</td>
    <td class="taxes">{{taxes}}</td>
    <td class="created">{{created}}</td>
    <td class="edit">
        <p data-placement="top" data-toggle="tooltip" title="Edit">
            <button class="btn btn-primary btn-xs" data-title="Edit" data-toggle="modal" data-target="#edit" >
                <span class="glyphicon glyphicon-pencil"></span>
            </button>
        </p>
    </td>
    <td class="delete">
        <p data-placement="top" data-toggle="tooltip" title="Delete">
            <button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" >
                <span class="glyphicon glyphicon-trash"></span>
            </button>
        </p>
    </td>
</tr>

{{/estates}}

    </tbody>
</table>
