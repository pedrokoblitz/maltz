<!--TICKETS-->

<table>
    <thead>
        <tr>
            <th>#</th>
            <th>dev</th>
            <th>project</th>
            <th>problem_url</th>
            <th>description</th>
            <th>activity</th>
            <th>created</th>
            <th>modified</th>
            <th>edit</th>
            <th>delete</th>
        </tr>
    </thead>
    <tbody>

{{#tickets}}

<tr id="record-1" class="record">
    <td class="checkbox"><input type="checkbox" class="checkthis" /></td>
    <td class="dev">{{dev}}</td>
    <td class="project">{{project}}</td>
    <td class="problem_url">{{problem_url}}</td>
    <td class="description">{{description}}</td>
    <td class="activity">{{activity}}</td>
    <td class="created">{{created}}</td>
    <td class="modified">{{modified}}</td>
    <td class="edit">
        <p data-placement="top" data-toggle="tooltip" title="Edit">
            <button class="btn btn-primary btn-xs" data-title="Edit" data-toggle="modal" data-target="#edit" >
                <span class="glyphicon glyphicon-pencil"></span>
            </button>
        </p>
    </td>
    <td class="delete">
        <p data-placement="top" data-toggle="tooltip" title="Delete">
            <button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" >
                <span class="glyphicon glyphicon-trash"></span>
            </button>
        </p>
    </td>
</tr>

{{/tickets}}

    </tbody>
</table>
